{
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES2017",
    "lib": [
      "DOM",
      "ES6",
      "ES2017",
      "esnext"
    ],
    "noImplicitAny": false,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "baseUrl": ".",
    "removeComments": false,
    "resolveJsonModule": true,
    "paths": {
      "foundry": [
        "types/foundry-pc-types"
      ]
    },
    "types": [
      "jquery",
      "webpack-env"
    ],
    "allowJS": true,
    "alwaysStrict": true
  }
}
