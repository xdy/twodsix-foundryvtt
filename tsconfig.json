{
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES2018",
    "lib": ["WebWorker", "ScriptHost", "ESNext", "DOM"],
    "noImplicitAny": false,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "baseUrl": ".",
    "removeComments": false,
    "resolveJsonModule": true,
    "types": ["jquery", "webpack-env", "foundry-pc-types"],
    "alwaysStrict": true
  }
}
